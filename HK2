local LabelInfo = Instance.new("TextLabel")
LabelInfo.BorderSizePixel = 0
LabelInfo.TextTransparency = 0.3
LabelInfo.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
LabelInfo.TextSize = 9
LabelInfo.FontFace = Font.new("rbxasset://fonts/families/Ubuntu.json", Enum.FontWeight.Medium, Enum.FontStyle.Normal)
LabelInfo.TextColor3 = Color3.fromRGB(255, 255, 255)
LabelInfo.BackgroundTransparency = 1
LabelInfo.Size = UDim2.new(0, 288, 0, 9)
LabelInfo.BorderColor3 = Color3.fromRGB(18, 18, 18)
LabelInfo.Name = "LabelInfo"
LabelInfo.Position = UDim2.new(0, 0, 0.65385, 0)
LabelInfo.Parent = LabelHolder

local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local dragging
local dragInput
local dragStart
local startPos

local function updateInput(input)
	local delta = input.Position - dragStart
	MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

MainFrame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = MainFrame.Position
		
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

MainFrame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		updateInput(input)
	end
end)

local uiVisible = true

UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if gameProcessed then return end
	
	if input.KeyCode == Enum.KeyCode.LeftAlt then
		uiVisible = not uiVisible
		MainFrame.Visible = uiVisible
	end
end)

local Library = {}

function Library:CreateWindow(name)
	Title.Text = name or "HK2"
	return Library
end

function Library:CreateTab(name)
	local newTab = TabButton:Clone()
	newTab.Text = name or "New Tab"
	newTab.Parent = TabContainerFrame
	
	local newPage = Page:Clone()
	newPage.Name = name or "NewTab"
	newPage.Parent = PageFolder
	newPage.Visible = false
	
	newTab.MouseButton1Click:Connect(function()
		for _, page in pairs(PageFolder:GetChildren()) do
			if page:IsA("ScrollingFrame") then
				page.Visible = false
			end
		end
		newPage.Visible = true
	end)
	
	if #PageFolder:GetChildren() == 1 then
		newPage.Visible = true
	end
	
	local TabFunctions = {}
	
	function TabFunctions:CreateButton(config)
		local newButtonHolder = ButtonHolder:Clone()
		local newButton = newButtonHolder.Button
		
		newButtonHolder.Parent = newPage
		newButton.Text = config.Text or "Button"
		
		if config.Callback then
			newButton.MouseButton1Click:Connect(function()
				config.Callback()
			end)
		end
		
		newButton.MouseEnter:Connect(function()
			TweenService:Create(newButton, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
		end)
		
		newButton.MouseLeave:Connect(function()
			TweenService:Create(newButton, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
		end)
		
		return newButton
	end
	
	function TabFunctions:CreateToggle(config)
		local newToggleHolder = ToggleHolder:Clone()
		local newToggleTitle = newToggleHolder.ToggleTitle
		local newToggleButton = newToggleHolder.ToggleButton
		local newToggleFrame = newToggleButton.ToggleFrame
		local newToggleBall = newToggleFrame.ToggleBall
		
		newToggleHolder.Parent = newPage
		newToggleTitle.Text = config.Text or "Toggle"
		
		local toggled = config.Default or false
		
		local function updateToggle()
			if toggled then
				TweenService:Create(newToggleBall, TweenInfo.new(0.2), {Position = UDim2.new(0.545, 0, 0.211, 0)}):Play()
				TweenService:Create(newToggleFrame, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(138, 247, 255)}):Play()
			else
				TweenService:Create(newToggleBall, TweenInfo.new(0.2), {Position = UDim2.new(0.206, 0, 0.211, 0)}):Play()
				TweenService:Create(newToggleFrame, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(6, 6, 6)}):Play()
			end
		end
		
		updateToggle()
		
		newToggleButton.MouseButton1Click:Connect(function()
			toggled = not toggled
			updateToggle()
			if config.Callback then
				config.Callback(toggled)
			end
		end)
		
		local ToggleFunctions = {}
		
		function ToggleFunctions:Set(value)
			toggled = value
			updateToggle()
			if config.Callback then
				config.Callback(toggled)
			end
		end
		
		return ToggleFunctions
	end
	
	function TabFunctions:CreateSlider(config)
		local newSliderHolder = SliderHolder:Clone()
		local newSliderTitle = newSliderHolder.SliderTitle
		local newSliderButton = newSliderHolder.SliderButton
		local newSliderTrail = newSliderButton.SliderTrail
		local newSliderNumber = newSliderHolder.SliderNumber
		
		newSliderHolder.Parent = newPage
		newSliderTitle.Text = config.Text or "Slider"
		
		local min = config.Min or 0
		local max = config.Max or 100
		local default = config.Default or min
		local value = default
		
		local function updateSlider(mouseX)
			local relativeX = math.clamp(mouseX - newSliderButton.AbsolutePosition.X, 0, newSliderButton.AbsoluteSize.X)
			local percentage = relativeX / newSliderButton.AbsoluteSize.X
			value = math.floor(min + (max - min) * percentage)
			newSliderTrail.Size = UDim2.new(percentage, 0, 1, 0)
			newSliderNumber.Text = tostring(value)
			
			if config.Callback then
				config.Callback(value)
			end
		end
		
		newSliderButton.MouseButton1Down:Connect(function()
			updateSlider(UserInputService:GetMouseLocation().X)
			
			local connection
			connection = RunService.RenderStepped:Connect(function()
				if UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1) then
					updateSlider(UserInputService:GetMouseLocation().X)
				else
					connection:Disconnect()
				end
			end)
		end)
		
		updateSlider(newSliderButton.AbsolutePosition.X + (default - min) / (max - min) * newSliderButton.AbsoluteSize.X)
		
		local SliderFunctions = {}
		
		function SliderFunctions:Set(newValue)
			value = math.clamp(newValue, min, max)
			local percentage = (value - min) / (max - min)
			newSliderTrail.Size = UDim2.new(percentage, 0, 1, 0)
			newSliderNumber.Text = tostring(value)
			
			if config.Callback then
				config.Callback(value)
			end
		end
		
		return SliderFunctions
	end
	
	function TabFunctions:CreateTextBox(config)
		local newTextBoxHolder = TextBoxHolder:Clone()
		local newTextBoxTitle = newTextBoxHolder.TextBoxTitle
		local newTextBox = newTextBoxHolder.TextBox
		
		newTextBoxHolder.Parent = newPage
		newTextBoxTitle.Text = config.Text or "TextBox"
		newTextBox.PlaceholderText = config.Placeholder or "Enter text..."
		
		if config.Callback then
			newTextBox.FocusLost:Connect(function(enterPressed)
				if enterPressed then
					config.Callback(newTextBox.Text)
				end
			end)
		end
		
		local TextBoxFunctions = {}
		
		function TextBoxFunctions:Set(text)
			newTextBox.Text = text or ""
		end
		
		return TextBoxFunctions
	end
	
	function TabFunctions:CreateDropdown(config)
		local newDropdownHolder = DropdownHolder:Clone()
		local newDropdownTitle = newDropdownHolder.DropdownTitle
		local newDropdownButton = newDropdownHolder.DropdownButton
		local newDropdownIcon = newDropdownButton.DropdownIcon
		local newDropdownContainer = newDropdownHolder.DropdownContainer
		local newDropdownOptionContainer = newDropdownContainer.DropdownOptionContainer
		
		newDropdownHolder.Parent = newPage
		newDropdownTitle.Text = config.Text or "Dropdown"
		
		local options = config.Options or {}
		local selected = config.Default
		local open = false
		
		local function updateDropdown()
			for _, option in pairs(newDropdownOptionContainer:GetChildren()) do
				if option:IsA("TextButton") then
					option:Destroy()
				end
			end
			
			for i, option in pairs(options) do
				local newOption = Option1:Clone()
				newOption.Text = option
				newOption.Parent = newDropdownOptionContainer
				
				newOption.MouseButton1Click:Connect(function()
					selected = option
					newDropdownTitle.Text = config.Text .. ": " .. selected
					open = false
					newDropdownContainer.Visible = false
					newDropdownOptionContainer.Visible = false
					
					if config.Callback then
						config.Callback(selected)
					end
				end)
			end
			
			newDropdownOptionContainer.CanvasSize = UDim2.new(0, 0, 0, #options * 27)
			newDropdownContainer.Size = UDim2.new(0, 288, 0, math.min(#options * 27 + 10, 100))
		end
		
		updateDropdown()
		
		if selected then
			newDropdownTitle.Text = config.Text .. ": " .. selected
		end
		
		newDropdownButton.MouseButton1Click:Connect(function()
			open = not open
			newDropdownContainer.Visible = open
			newDropdownOptionContainer.Visible = open
		end)
		
		local DropdownFunctions = {}
		
		function DropdownFunctions:Refresh(newOptions)
			options = newOptions or {}
			updateDropdown()
		end
		
		function DropdownFunctions:Set(value)
			selected = value
			newDropdownTitle.Text = config.Text .. ": " .. selected
			if config.Callback then
				config.Callback(selected)
			end
		end
		
		return DropdownFunctions
	end
	
	function TabFunctions:CreateLabel(config)
		local newLabelHolder = LabelHolder:Clone()
		local newLabelTitle = newLabelHolder.LabelTitle
		local newLabelInfo = newLabelHolder.LabelInfo
		
		newLabelHolder.Parent = newPage
		newLabelTitle.Text = config.Text or "Label"
		newLabelInfo.Text = config.Info or ""
		
		local LabelFunctions = {}
		
		function LabelFunctions:Set(text)
			newLabelTitle.Text = text or ""
		end
		
		function LabelFunctions:SetInfo(info)
			newLabelInfo.Text = info or ""
		end
		
		return LabelFunctions
	end
	
	return TabFunctions
end

return Library
